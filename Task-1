function multiplyAllArguments() {
  let result = 1;
  for (let i = 0; i < arguments.length; i++) {
    result *= arguments[i];
  }
  return result;
}

function sumAllNumbers(...nums) {
  return nums.reduce((total, num) => total + num, 0);
}
console.log(greetUser("Kelvin", 25));                 
console.log(multiplyAllArguments(2, 4, 6));         
console.log(sumAllNumbers(1, 2, 3, 4, 5));
